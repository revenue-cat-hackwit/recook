{
  "expo": {
    "name": "Pirinku",
    "slug": "pirinku",
    "scheme": "pirinku",
    "version": "1.0.0",
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-router"],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hackwit.pirinku"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png"
      },
      "package": "com.hackwit.pirinku"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01fe9c61-ad2a-47d1-93ee-8f5a160633ac"
      }
    },
    "owner": "hackwit"
  }
}
